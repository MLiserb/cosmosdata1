[["/home/project/index.html","<!DOCTYPE html>\n<html lang=\"en\">\n  <head>\n    <meta charset=\"UTF-8\" />\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n    <title>Cosmos Blocks Chart</title>\n    <style>\n      body {\n        font-family: Arial, sans-serif;\n        display: flex;\n        flex-direction: column;\n        min-height: 100vh;\n        margin: 0;\n      }\n      #app {\n        max-width: 800px;\n        margin: 0 auto;\n        padding: 20px;\n        flex-grow: 1;\n      }\n      .controls {\n        display: flex;\n        flex-direction: column;\n        align-items: flex-start;\n        margin-bottom: 20px;\n      }\n      .control-item {\n        margin-bottom: 10px;\n      }\n      input[type=\"date\"] {\n        padding: 5px;\n      }\n      button {\n        padding: 10px 20px;\n        font-size: 16px;\n        cursor: pointer;\n      }\n      .scale-toggle {\n        margin-top: 10px;\n      }\n      footer {\n        text-align: center;\n        padding: 10px;\n        background-color: #f0f0f0;\n      }\n    </style>\n  </head>\n  <body>\n    <div id=\"app\">\n      <h1>Cosmos Blocks per Day</h1>\n      <div class=\"controls\">\n        <div class=\"control-item\">\n          <label for=\"startDate\">Start Date:</label>\n          <input type=\"date\" id=\"startDate\">\n        </div>\n        <div class=\"control-item\">\n          <label for=\"endDate\">End Date:</label>\n          <input type=\"date\" id=\"endDate\">\n        </div>\n        <div class=\"control-item\">\n          <button id=\"reloadButton\">Reload Data</button>\n        </div>\n      </div>\n      <div class=\"scale-toggle\">\n        <label>\n          <input type=\"checkbox\" id=\"logScale\"> Use Logarithmic Scale\n        </label>\n      </div>\n      <canvas id=\"myChart\" width=\"800\" height=\"400\"></canvas>\n    </div>\n    <footer>\n      <p>&copy; 2024 MaFi</p>\n    </footer>\n    <script type=\"module\" src=\"/src/main.js\"></script>\n  </body>\n</html>"]]